# Copy this file to `.env` and set real values before running docker compose.
# `.env` is git-ignored and must never be committed.

# Auth DB
POSTGRES_AUTH_USER=wrenchit_auth_user
POSTGRES_AUTH_PASSWORD=change_me_auth_db_password
POSTGRES_AUTH_DB=wrenchit_auth

# App DB
POSTGRES_APP_USER=wrenchit_app_user
POSTGRES_APP_PASSWORD=change_me_app_db_password
POSTGRES_APP_DB=wrenchit_app

# Keycloak DB
POSTGRES_KC_USER=wrenchit_kc_user
POSTGRES_KC_PASSWORD=change_me_keycloak_db_password
POSTGRES_KC_DB=wrenchit_keycloak

# Keycloak admin
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=change_me_keycloak_admin_password
KEYCLOAK_DEMO_USER_PASSWORD=change_me_demo_user_password
KEYCLOAK_DEMO_ADMIN_PASSWORD=change_me_demo_admin_password

# API / security behavior
WRENCHIT_SECURITY_AUTH_ENABLED=true
WRENCHIT_GOOGLE_ENABLED=false

# Optional external API key (backend Google Places integration)
GOOGLE_PLACES_API_KEY=

# Optional browser maps key injected into www/public/config.js at image build time
WRENCHIT_FRONTEND_GOOGLE_MAPS_API_KEY=

# JWT issuer config consumed by API service
KEYCLOAK_CLIENT_ID=wrenchit-api-public
KEYCLOAK_ISSUER_URI=http://keycloak:8082/realms/wrenchit
KEYCLOAK_JWK_SET_URI=http://keycloak:8082/realms/wrenchit/protocol/openid-connect/certs
KEYCLOAK_ADMIN_REALM=master
KEYCLOAK_USER_ROLE=USER

# Infra ports
CLAMAV_PORT=3310
