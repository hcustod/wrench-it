{
  "realm": "wrenchit",
  "enabled": true,
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "displayName": "Wrench It Realm",
  "roles": {
    "realm": [
      {
        "name": "USER",
        "description": "Standard API user role"
      },
      {
        "name": "ADMIN",
        "description": "Administrative API role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "wrenchit-api-public",
      "name": "wrenchit-api-public",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "bearerOnly": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "redirectUris": [
        "http://localhost:8081/*",
        "http://localhost:3000/*",
        "http://localhost:5173/*"
      ],
      "webOrigins": [
        "http://localhost:8081",
        "http://localhost:3000",
        "http://localhost:5173"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:8081/*"
      }
    }
  ],
  "users": [
    {
      "username": "api-user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "API",
      "lastName": "User",
      "email": "api-user@wrenchit.local",
      "credentials": [
        {
          "type": "password",
          "value": "__KEYCLOAK_DEMO_USER_PASSWORD__",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER"
      ]
    },
    {
      "username": "api-admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "API",
      "lastName": "Admin",
      "email": "api-admin@wrenchit.local",
      "credentials": [
        {
          "type": "password",
          "value": "__KEYCLOAK_DEMO_ADMIN_PASSWORD__",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER",
        "ADMIN"
      ]
    }
  ]
}
